<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DynamicNumImageView">
        <attr name="android:thumb"/>
        <attr name="vertex_position" format="enum">
            <enum name="top_right" value="0" />
            <enum name="top_left" value="1" />
            <enum name="bottom_right" value="2" />
            <enum name="bottom_left" value="3" />
        </attr>
        <attr name="number_view_backgroud_color" format="color"/>
        <attr name="number_view_scale" format="float" />
        <attr name="text_content" format="string"/>
        <attr name="number_text_content_scale" format="float"/>
        <attr name="number_text_content_color" format="color"/>
    </declare-styleable>

</resources>